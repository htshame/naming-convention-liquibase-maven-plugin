<rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../../docs/schema/ruleset/1.0/liquibase-naming-convention-1.0.xsd">

    <rule name="no-hyphens-in-attributes">
        <excludedAncestorAttrs>
            <attr>defaultValue</attr>
            <attr>defaultValueComputed</attr>
            <attr>xmlns:xsi</attr>
            <attr>xsi:schemaLocation</attr>
        </excludedAncestorAttrs>
    </rule>
</rules>

